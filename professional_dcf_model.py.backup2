                validation_report['calculations_found'].append('sensitivity_analysis')
            else:
                validation_report['calculations_missing'].append('sensitivity_analysis')

        except Exception as e:
            validation_report['errors'].append(f"Sensitivity validation failed: {e}")

        # Calculate completeness score
        total_expected = len(expected_calculations) if expected_calculations else 8  # Default expectations
        total_found = len(validation_report['calculations_found'])
        validation_report['completeness_score'] = total_found / total_expected if total_expected > 0 else 0

        # Determine status
        if validation_report['completeness_score'] >= 0.9:
            validation_report['status'] = 'COMPLETE'
        elif validation_report['completeness_score'] >= 0.7:
            validation_report['status'] = 'MOSTLY_COMPLETE'
        elif validation_report['completeness_score'] >= 0.5:
