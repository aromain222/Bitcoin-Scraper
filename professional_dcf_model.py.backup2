    try:
        financials = source_data['financials']
        if 'series' in financials and 'annual' in financials['series']:
            annual_data = financials['series']['annual']
            if metric == 'Revenue':
                if 'revenue' in annual_data:
                    return annual_data['revenue'][0]['v'] if annual_data['revenue'] else None
            elif metric == 'EBITDA':
                if 'ebitda' in annual_data:
                    return annual_data['ebitda'][0]['v'] if annual_data['ebitda'] else None
            elif metric == 'Net Income':
                if 'netIncome' in annual_data:
                    return annual_data['netIncome'][0]['v'] if annual_data['netIncome'] else None
    except:
        pass
    return None

def extract_yahoo_metric(source_data, metric, years):
