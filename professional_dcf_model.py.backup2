            if 'present value' in valuation_text:
                validation_report['calculations_found'].append('discounting')
            else:
                validation_report['calculations_missing'].append('discounting')

        except Exception as e:
            validation_report['errors'].append(f"DCF Valuation validation failed: {e}")

        # Check Sensitivity tab
        try:
            ws_sensitivity = sh.worksheet("Sensitivity")
            sensitivity_data = ws_sensitivity.get_all_values()
