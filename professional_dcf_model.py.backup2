def cross_validate_financial_data(data_sources, years):
    """
    Cross-validate data from multiple sources and return the most reliable values.
    """
    result = {}

    # Extract and compare key metrics
    metrics = ['Revenue', 'EBITDA', 'Net Income', 'Total Assets', 'Total Debt', 'Cash']

    for metric in metrics:
        values = []
        confidences = []

        # Extract values from each source
        for source_name, source_data in data_sources.items():
            if source_name == 'alpha_vantage':
                value = extract_alpha_vantage_metric(source_data, metric, years)
            elif source_name == 'finnhub':
                value = extract_finnhub_metric(source_data, metric, years)
            elif source_name == 'yahoo':
                value = extract_yahoo_metric(source_data, metric, years)
            else:
                continue

            if value is not None:
                values.append(value)
                confidences.append(get_source_confidence(source_name))

        # Select most reliable value
        if values:
            result[metric] = select_most_reliable_value(values, confidences)

    # Add company info from most reliable source
    result.update(extract_company_info(data_sources))

    return result

def get_source_confidence(source_name):
    """Return confidence score for each data source (0-1)."""
    confidence_scores = {
        'alpha_vantage': 0.95,  # Most accurate
        'finnhub': 0.90,       # Real-time but slightly less comprehensive
        'yahoo': 0.80,         # Broad coverage but some inconsistencies
        'sec_edgar': 0.98      # Official but harder to parse
    }
    return confidence_scores.get(source_name, 0.5)

def select_most_reliable_value(values, confidences):
    """Select the most reliable value using confidence-weighted average."""
    if len(values) == 1:
        return values[0]

    # Weight by confidence and calculate weighted average
    total_weight = sum(confidences)
    weighted_sum = sum(v * c for v, c in zip(values, confidences))

    return weighted_sum / total_weight if total_weight > 0 else values[0]

def calculate_data_quality_score(data_sources):
    """Calculate overall data quality score (0-100)."""
    base_score = 0
    source_bonus = len(data_sources) * 15  # Bonus for multiple sources
    recency_bonus = 10  # Assuming data is recent

    # Source quality bonuses
    quality_multipliers = {
        'alpha_vantage': 1.2,
        'sec_edgar': 1.1,
        'finnhub': 1.0,
        'yahoo': 0.9
    }

    total_multiplier = sum(quality_multipliers.get(source, 0.8) for source in data_sources.keys())
    total_multiplier /= len(data_sources)

    return min(100, (base_score + source_bonus + recency_bonus) * total_multiplier)

def extract_alpha_vantage_metric(source_data, metric, years):
    """Extract specific metric from Alpha Vantage data."""
    try:
        if metric == 'Revenue':
            df = source_data['income']
            if 'totalRevenue' in df.columns:
                values = df['totalRevenue'].head(years).tolist()
                return values[0] if values else None
        elif metric == 'EBITDA':
            df = source_data['income']
            if 'ebitda' in df.columns:
                values = df['ebitda'].head(years).tolist()
                return values[0] if values else None
        elif metric == 'Net Income':
            df = source_data['income']
            if 'netIncome' in df.columns:
                values = df['netIncome'].head(years).tolist()
                return values[0] if values else None
        elif metric == 'Total Assets':
            df = source_data['balance']
            if 'totalAssets' in df.columns:
                values = df['totalAssets'].head(years).tolist()
                return values[0] if values else None
        elif metric == 'Total Debt':
            df = source_data['balance']
            if 'totalLiabilities' in df.columns and 'totalCurrentLiabilities' in df.columns:
                values = (df['totalLiabilities'] - df['totalCurrentLiabilities']).head(years).tolist()
                return values[0] if values else None
        elif metric == 'Cash':
            df = source_data['balance']
            if 'cashAndCashEquivalentsAtCarryingValue' in df.columns:
                values = df['cashAndCashEquivalentsAtCarryingValue'].head(years).tolist()
                return values[0] if values else None
    except:
        pass
    return None

